{
  "name": "js-babel-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for JS projects with Gulp set to rebuild src/ contents as es5 in dist/. Update project.json and gulpfile.js before use in new projects.",
  "main": "index.js",
  "scripts": {
    "dev": "babel src --out-dir dist --presets es2015 --source-maps inline --watch",
    "build": "babel src --out-dir dist --presets es2015 --source-maps inline"
  },
  "keywords": [
    "js",
    "gulp",
    "babel",
    "boilerplace",
    "es6",
    "es5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LeonidasKu/js-babel-boilerplate.git"
  },
  "author": "Leonid Kukuyev <leonidkukuyev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "source-map-support": "^0.4.0"
  }
}
